void displayedge( int arr[100][100])
{
    for(int i=0;i<36;i++)
    {
        for(int j=0;j<36;j++)
        {
            if(graph[i][j]!=0)
                printf("%d->%d\n",i,j);
        }
    }
}


void bubble_sort(struct student arr[], int n)
{
    struct student temp;
    for (int i = 0; i < n-1; i++) {
        for (int j = 0; j < n-i-1; j++) {
            if (arr[j].rank > arr[j+1].rank) {
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
}

void displayStudent(struct student s[],int n)
{
    for (int i = 0; i < n; i++)
    {
        printf("%s:   \t%d   \t%s   \t%d   \n", s[i].name,s[i].roll,s[i].email ,s[i].rank);
    }
}

void displayGraph(int graph[100][100])
{
    int i,j;
    for(i=0;i<36;i++)
        {   for(j=0;j<36;j++)
            {
            printf(" %d ",graph[i][j]);

            }
            printf("\n");
        }
}


