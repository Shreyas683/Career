struct student
{
    char name[20];
    int rank;
};
int main()
{
    int i,j,key;
     struct student s[20]={
    {"Bond007",10067},
    {"Neha",99678},
    {"Ananya",21203},
    {"Shreyas",63634},
    {"Amogh",7802},
    {"Jadi",56783},
    {"Sameer",24658},
    {"Shraddha",11508},
    {"Shiggavi",94828},
    {"Punith",37377},
    {"Kathik",87828},
    {"Sunil",46966},
    {"Praveen",64725},
    {"Basappa",2475},
    {"Pragati",567},
    {"Dabade",28465},
     };


    while(1)
    {
        printf("1. Display Edges\n");
        printf("2. Sort the rank\n");
        scanf("%d",&key);
        switch(key)
        {
            case 1: displayedge(graph);
                    break;

            case 2: bubble_sort(s, 15);
                    displayStudent(s,16);
                    break;

            default: exit(0);
        }
    }
}

void displayedge( int arr[36][36])
{
    for(int i=0;i<36;i++)
    {
        for(int j=0;j<36;j++)
        {
            if(graph[i][j]!=0)
                printf("%d->%d\n",i,j);
        }
    }
}


void bubble_sort(struct student arr[], int n)
{
    struct student temp;
    for (int i = 0; i < n-1; i++) {
        for (int j = 0; j < n-i-1; j++) {
            if (arr[j].rank > arr[j+1].rank) {
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
}

void displayStudent(struct student s[],int n)
{
    for (int i = 0; i < n; i++) {
        printf("%s:     \t%d\n", s[i].name, s[i].rank);
    }
}
