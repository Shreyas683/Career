struct student
{
    char name[20];
    int rank;
};
int main()
{
    printf("0--->10th\t");
    printf("1--->Diploma\t");
    printf("2--->Government_service\t");
    printf(" 3--->Science\n");
    printf("4--->Commerce\t");
    printf("5--->Arts\t");
    printf("6--->B.Arch\t");
    printf("7--->B.Sc\n");
    printf("8--->B.Pharma\t");
    printf("9--->B.E/B.Tech\t");
    printf("10-->BAMS\t");
    printf("11-->MBBS\n");
    printf("12-->BDS\t");
    printf("13-->B.Com\t");
    printf("14-->BBA\t");
    printf("15-->CA\n");
    printf("16-->BSW\t");
    printf("17-->BA\t");
    printf("18-->LLB\t");
    printf("19-->LLM\n");
    printf("20-->MA\t");
    printf("21-->MSW\t");
    printf("22-->BCA\t");
    printf("23-->MCA\n");
    printf("24-->MBA\t");
    printf("25-->M.Com\t");
    printf("26-->B.Ed\t");
    printf("27-->M.Ed\n");
    printf("28-->CS\t");
    printf("29-->MDS\n");
    printf("30-->M.D\n");
    printf("31-->M.Tech\n");
    printf("32-->M.Pharma\n");
    printf("33-->M.Sc");
    printf("34-->P.hd\n");

    int i,j,key;
     struct student s[20]={
    {"Bond007",10067},
    {"Neha",99678},
    {"Ananya",21203},
    {"Shreyas",63634},
    {"Amogh",7802},
    {"Jadi",56783},
    {"Sameer",24658},
    {"Shraddha",11508},
    {"Shiggavi",94828},
    {"Punith",37377},
    {"Kathik",87828},
    {"Sunil",46966},
    {"Praveen",64725},
    {"Basappa",2475},
    {"Pragati",567},
    {"Dabade",28465},
     };


    while(1)
    {
        printf("\n\n");

        printf("1. Display Edges\n");
        printf("2. Sort the rank\n");
        printf("3. Display Graph\n");
        printf("Enter the choice:");
        scanf("%d",&key);
        switch(key)
        {
            case 1: displayedge(graph);
                    break;

            case 2: bubble_sort(s, 15);
                    displayStudent(s,16);
                    break;

            case 3: displayGraph(graph);

            default: exit(0);
        }
    }
}

void displayedge( int arr[36][36])
{
    for(int i=0;i<36;i++)
    {
        for(int j=0;j<36;j++)
        {
            if(graph[i][j]!=0)
                printf("%d->%d\n",i,j);
        }
    }
}


void bubble_sort(struct student arr[], int n)
{
    struct student temp;
    for (int i = 0; i < n-1; i++) {
        for (int j = 0; j < n-i-1; j++) {
            if (arr[j].rank > arr[j+1].rank) {
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
}

void displayStudent(struct student s[],int n)
{
    for (int i = 0; i < n; i++) {
        printf("%s:     \t%d\n", s[i].name, s[i].rank);
    }
}

displayGraph(int graph[100][100])
{
    int i,j;
    for(i=0;i<36;i++)
        {   for(j=0;j<36;j++)
            {
            printf(" %d ",graph[i][j]);

            }
            printf("\n");
        }
}
